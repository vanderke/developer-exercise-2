<div class="container">
    <div ng-model="myData">
        <div class="row">
            <div ng-repeat="pillar in Pillars">
                <div class=".col-xs-12 .col-sm-6 col-md-3 .col-lg-3 css-block pillar">
                    <h2>{{pillar.Title}}</h2>
                    <p>{{pillar.Description}}</p>
                    <p><em>{{pillar.Scripture}}</em></p>
                </div>
            </div>
        </div>
    </div>
</div>
